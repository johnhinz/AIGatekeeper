{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "Console"
      },
      {
        "Name": "File",
        "Args": {
          "path": "log/log.txt",
          "rollingInterval": "Day",
          "retainedFileCountLimit": 14
        }
      }
    ],
    "Enrich": [
      "FromLogContext",
      "WithMachineName"
    ],
    "Properties": {
      "Application": "AIMS"
    }
  },
  "AIEndpoint": "http://vmhost.johnhinz.com:80/v1/vision/detection",
  "PublisherName": "AIPublisher",
  "TopicParser": "\\.",
  "TopicPosition": 0,
  "QueueName": "AI",
  "WatchFolder": "c:\\temp",
  //"LogFile":  "c:\\temp\\AIGuardianLog.txt",
  "WatchedExtensions": "*.jpg;*.jpeg",
  "RepositoryEndpoint": "vmhost.johnhinz.com",
  "Cameras": [
    {
      "name": "east",
      "clip": false,
      "watches": [
        {
          "label": "person",
          "confidence": 0.75
        }
      ]
    },
